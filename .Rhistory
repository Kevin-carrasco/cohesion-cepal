conf_institucional_fa<-
data %>%
dplyr::select(gobierno = c05_01_w01,
partidos = c05_02_w01,
carabineros = c05_03_w01,
sindicatos = c05_04_w01,
judicial = c05_05_w01,
empresas = c05_06_w01,
congreso = c05_07_w01,
presidente = c05_08_w01)
psych::fa.parallel(conf_institucional_fa, fa="fa")
psych::fa.parallel(conf_institucional_fa, fa="fa")
psych::scree(conf_institucional_fa)
psych::fa.parallel(conf_institucional_fa, fa="fa")
psych::scree(conf_institucional_fa)
fac_ml <- psych::fa(r = conf_institucional_fa, nfactors = 3, fm= "ml", rotate = "varimax" )
fac_ml$loadings
source("processing/fa_table.R")
inst_fa <-fa_table(fac_ml, .32)
knitr::kable(inst_fa, "html") %>%
kable_styling(bootstrap_options = c("striped", "hover")) %>%
cat(., file = "bookdown-files/output/tables/inst_fa.html")
webshot(url ="bookdown-files/output/tables/inst_fa.html", file ="bookdown-files/output/tables/inst_fa.png", vheight = 10,  vwidth = 10)
